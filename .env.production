# BuildTrack Pro Production Environment Configuration
# This file is used in production builds like CI/CD pipelines

# Feature flags for BuildTrack Pro
NEXT_PUBLIC_FEATURE_MOCK_IMAGES=true

# Skip connecting to Supabase during build time
SKIP_SUPABASE_INIT_ON_BUILD=true

# Add buildtime placeholders that will be replaced by actual values in the CI environment
NEXT_PUBLIC_SUPABASE_URL=https://placeholder-for-ci.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=placeholder-key-replaced-in-ci
